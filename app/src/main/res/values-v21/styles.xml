<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
    </style>

    <!--regcris-->
    <style name="RegcrisTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryReg</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkReg</item>
        <item name="colorAccent">@color/colorAccentReg</item>
        <item name="android:statusBarColor">@color/colorPrimaryReg</item>
    </style>

    <!--prestige-->
    <style name="PrestigeTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryPres</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkPres</item>
        <item name="colorAccent">@color/colorAccentPres</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:statusBarColor">@color/colorPrimaryPres</item>
    </style>

    <!--tmarks-->
    <style name="TmarksTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryTM</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkTM</item>
        <item name="colorAccent">@color/colorAccentTM</item>
        <item name="android:statusBarColor">@color/colorPrimaryTM</item>
    </style>
    
</resources>